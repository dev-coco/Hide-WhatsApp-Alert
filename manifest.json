{
  "manifest_version": 2,
  "name": "WhatsApp 屏蔽弹窗",
  "description": "WhatsApp 屏蔽弹窗。",
  "author": "dev-coco",
  "version": "1.0.0",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
       "https://web.whatsapp.com/*"
      ],
        "css": ["inject.css"]
    }
  ],
  "permissions": [
    "https://web.whatsapp.com/*"
  ]
}